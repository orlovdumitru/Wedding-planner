
<h3>@ViewBag.wedding.wedderOne and @ViewBag.wedding.wedderTwo&#39;s </h3>
<p style="margin-left:65%; margin-top: -40px;">
    <a asp-area="" asp-controller="Home" asp-action="AllWeddings">Dashboard</a>
    &nbsp; | &nbsp;
    <a asp-area="" asp-controller="Home" asp-action="LogOut">Log Out</a>
</p>

<div style="width:29%; display:inline-block; vertical-align:top;">
    <h4>Date: @ViewBag.wedding.created_at</h4>
    <br>
    <h4>Guests: </h4>
    @foreach(var guest in @ViewBag.weddings){
        <p>@guest.user.first_name @guest.user.last_name</p>
        
    }
</div>

